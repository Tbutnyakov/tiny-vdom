{
  "compilerOptions": {
    "outDir": "./dist/",
    "baseUrl": "./",
    "strict": true,
    "jsx": "react",
    "jsxFactory": "makeElement",
    "target": "es5",
    "allowJs": true,
    "lib": ["dom", "es2019"],
    "esModuleInterop": true,
    "downlevelIteration": true,
    "keyofStringsOnly": true
  },
  "include": ["src/**/*"]
}
